{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", { "endOfLine":"auto" }],
        "arrow-parens": "off",
        "no-console": "error",
        "comma-dangle": "off",
        "indent": "off",
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-tabs": "off",
        "react/function-component-definition": [
            2,
            {
                "namedComponents": "arrow-function"
            }
        ],
        "no-param-reassign": "off",
        "react/jsx-indent": [
            2,
            "tab"
        ],
        "react/jsx-indent-props": [
            2,
            "tab"
        ],
        "no-prototype-builtins": "off",
        "react/prop-types": "off"
    }
}